send seq1 = TESTSEQ::AttachStimulusSequence( caseId:tc, seqLabel:"seq_a" );
stId = StimulusA::Create( seq:seq1, value:15 );
ObservationY::CreatePolling( stimulus:stId, expectedC:False, pollInterval:2, timeLimit:0 );

bucket.Initialized();',
